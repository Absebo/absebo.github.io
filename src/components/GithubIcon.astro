---
import { Image } from "astro:assets";

const { invert = false, alt, ...props } = Astro.props;
---

<Image
    src="https://raw.githubusercontent.com/simple-icons/simple-icons/develop/icons/github.svg"
    style={invert ? `filter: invert();` : ""}
    alt={alt}
    width="32"
    height="32"
    priority
    {...props}
/>
